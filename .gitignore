# Ignore compiled Java and build artifacts
target/
build/

# Ignore Maven wrapper files (except the jar)
!.mvn/wrapper/maven-wrapper.jar

# Ignore IntelliJ and VS Code project settings
.idea/
.vscode/

# Ignore IDE-specific files
*.iml
*.ipr
*.iws
.classpath
.factorypath
.project
.settings/
.springBeans/
.sts4-cache/

# Ignore NetBeans project files
/nbproject/private/
/nbbuild/
/dist/
/nbdist/
/.nb-gradle/

# Ignore application.properties (contains credentials)
src/main/resources/application.properties

# Ignore STS (Spring Tool Suite) generated files
.apt_generated

# Keep Maven and Gradle build directories ignored
!**/src/main/**/build/
!**/src/test/**/build/

